<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net45</TargetFrameworks>
    <AssemblyName>NPOI.ToEnumerable</AssemblyName>
    <RootNamespace>NPOI.ToEnumerable</RootNamespace>

    <Authors>ychsu.tw</Authors>
    <Description>從iworkbook取資料到 IEunmerable&lt;T&gt;</Description>
    <PackageProjectUrl>https://github.com/ychsu/NPOI.ToEnumerable</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ychsu/NPOI.ToEnumerable</RepositoryUrl>
    <Product>NPOI.ToEnumerable</Product>
    <PackageId>NPOI.ToEnumerable</PackageId>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageReleaseNotes>支援多框架</PackageReleaseNotes>
    <Version>1.3.0</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Company>ychsu.tw</Company>
    <PackageLicenseExpression>
      MIT
    </PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NPOI" Version="2.4.1" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>
</Project>